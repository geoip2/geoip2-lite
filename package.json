{
  "name": "geoip2-lite",
  "version": "1.0.2",
  "description": "MaxMind's GeoIP2 API implementation on Native NodeJS",
  "main": "lib/geoip2.js",
  "scripts": {
    "pretest": "eslint .",
    "test": "nodeunit --reporter=minimal test/tests.js",
    "updatedb": "node scripts/updatedb.js",
    "updatedb-debug": "node scripts/updatedb.js debug",
    "updatedb-force": "node scripts/updatedb.js force"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geoip2/geoip2-lite.git"
  },
  "keywords": [
    "geolocation",
    "geodata",
    "geoip",
    "geoip2",
    "ipv4",
    "ipv6",
    "geolookup",
    "geoip2-lite",
    "maxmind",
    "maxmind-api",
    "node-geoip",
    "node-geoip2",
    "geoip-lite",
    "geolite",
    "geoip2lite",
    "geoiplite",
    "ip",
    "geoip lookup",
    "geo country",
    "geo state",
    "geo city"
  ],
  "author": "Chandra Gupta <i@chandragupta.me> (https://chandragupta.me/)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/geoip2/geoip2-lite/issues"
  },
  "homepage": "https://github.com/geoip2/geoip2-lite",
  "engines": {
    "node": ">=5.10.0"
  },
  "dependencies": {
    "async": "2.1 - 2.6.4",
    "chalk": "4.1 - 4.1.2",
    "iconv-lite": "0.4.13 - 0.6.3",
    "ip-address": "5.8.9 - 5.9.4",
    "lazy": "1.0.11",
    "rimraf": "2.5.2 - 2.7.1",
    "yauzl": "2.9.2 - 2.10.0"
  },
  "config": {
    "update": true
  },
  "devDependencies": {
    "eslint": "^8.21.0"
  }
}
